<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CircuitSimulator.Views"
             xmlns:WPFHelloWorld="clr-namespace:WPFHelloWorld" x:Class="CircuitSimulator.Views.ResistorView"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Rectangle x:Name="resistor" Height="50" Width="25" Fill="Black" 
               MouseMove="Resistor_MouseMove" 
               IsHitTestVisible="{Binding ElementName=root, Path=IsChildHitTestVisible}"
               Cursor="Hand">
    </Rectangle>
</UserControl>
